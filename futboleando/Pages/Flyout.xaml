<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:futboleando.Pages"
             x:Class="futboleando.Pages.Flyout"
             >

    <FlyoutPage.Flyout>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage BarBackgroundColor="{StaticResource rojoOscuro}" BarTextColor="{StaticResource colorblanco}" x:Name="Navigate" >
            <x:Arguments>
                <pages:PrincipalPage></pages:PrincipalPage>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>